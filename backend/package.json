{
  "name": "norma-db",
  "version": "1.0.0",
  "description": "Database Normalization Analyzer - Version 1",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "cli": "ts-node src/cli/normadb.ts",
    "build:frontend": "cd ../frontend && npm run build",
    "install:frontend": "cd ../frontend && npm install",
    "dev:frontend": "cd ../frontend && npm start"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pgsql-ast-parser": "^12.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "database",
    "normalization",
    "sql",
    "postgresql",
    "analyzer"
  ],
  "author": "",
  "license": "MIT"
}
